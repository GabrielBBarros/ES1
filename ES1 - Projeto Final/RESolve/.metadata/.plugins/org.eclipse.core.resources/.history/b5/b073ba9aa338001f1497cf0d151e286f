package Model;

public class Bebida {
    private String nome;
    private double preco;
    private Estoque estoque;
    private Bar bar;

    public Bebida(String nome, double preco, Estoque estoque, Bar bar) {
        this.nome = nome;
        this.preco = preco;
        this.estoque = estoque;
        this.bar = bar;
    }

    public String getNome() {
        return nome;
    }

    public double getPreco() {
        return preco;
    }

    public Estoque getEstoque() {
        return estoque;
    }

    public Bar getBar() {
        return bar;
    }
}