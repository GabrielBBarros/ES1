package Model;

import java.util.List;
import java.util.ArrayList;

public class Cozinha {
    private List<Prato> pratos;

    public Cozinha() {
        this.pratos = new ArrayList<>();
    }

    public void addPrato(Prato prato) {
        pratos.add(prato);
    }
    
    public void removerPrato(Prato prato) {
        pratos.remove(prato);
    }

}
